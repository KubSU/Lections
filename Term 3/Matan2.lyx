\begin_layout Definition
\begin_inset Formula $\forall\varepsilon>0\ \exists N=N\left(\varepsilon\right)\ \forall n\br N\ \left|S\left(x\right)-S_{n}\left(x\right)\right|<\varepsilon\ \forall x\in D$
\end_inset


\end_layout

\begin_layout Definition
\begin_inset Formula $\left|S\left(x\right)-S_{n}\left(x\right)\right|<\varepsilon\ \forall x\in D\ \dsl\ \left|r_{n}\left(x\right)<\varepsilon\right|\ \forall x\in D$
\end_inset

 
\begin_inset Formula $\dsl$
\end_inset

 
\begin_inset Formula $\un{\sup}{x\in D}\left|r_{n}\left(x\right)\right|\mr\varepsilon$
\end_inset

 
\end_layout

