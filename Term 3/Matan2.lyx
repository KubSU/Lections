\end_inset


\begin_inset Formula $f_{n}\left(x\right)=\df xn$
\end_inset

, 
\begin_inset Formula $X=\R$
\end_inset

 --- бесконечное количество опредлеленных функций с номерами от 
\begin_inset Formula $1$
\end_inset

 до 
\begin_inset Formula $\b$
\end_inset


\end_layout

\begin_layout --Separator--

\end_layout

\begin_layout Example
\begin_inset Argument
status open

\begin_layout Plain Layout
2
\end_layout

\end_inset


\begin_inset Formula $f_{n}\left(x\right)=\df{\sin nx}{n^{2}},\ X\in\R$
\end_inset


\end_layout

\begin_layout --Separator--

\end_layout

