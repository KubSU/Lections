\end_inset

.
\end_layout

\begin_layout Standard
Пусть область 
\begin_inset Formula $D$
\end_inset

 --- правильная по 
\begin_inset Formula $y$
\end_inset

, ограничена линиями 
\begin_inset Formula $x=a,\ x=b,\ y=y_{1}\left(x\right),\ y=y_{0}\left(x\right),\ y_{1}\left(x\right)\br y_{0}\left(x\right),\ x\in\left[a;b\right]$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Note Note
status open

\begin_layout Plain Layout
Рис 75
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Пусть функция 
\begin_inset Formula $P\left(x,y\right)$
\end_inset

 определена и непрерывна в области 
\begin_inset Formula $D$
\end_inset

 вместе с часной производной 
\begin_inset Formula $P_{y}\pr\left(x,y\right)$
\end_inset

, тогда 
\begin_inset Formula 
\begin{eqnarray*}
\un{\iint}D\df{\dd P\left(x,y\right)}{\dd y}dxdy & = & \oui abdx\oui{y_{0}\left(x\right)}{y_{1}\left(x\right)}P_{y}\pr\left(x,y\right)=\\
 & = & \left\{ \text{По формуле Ньютона-Лейбница}\right\} =\\
 & = & \oui abP\left(x,y\right)|_{y_{0}\left(x\right)}^{y_{1}\left(x\right)}dx=\\
 & = & \oui abP\left(x,y_{1}\left(x\right)\right)dx-\oui abP\left(x,y_{0}\left(x\right)\right)dx=\\
 & = & \left\{ \begin{array}{c}
\left(x,y_{1}\left(x\right)\right)\in BC\\
\left(x,y_{0}\left(x\right)\right)\in AD
\end{array}\right\} =\\
 & = & \ui{BC}P\left(x,y\right)dx-\ui{AD}P\left(x,y\right)dx=\\
 & = & -\left(\ui{AD}P\left(x,y\right)dx+\ui{DC}P\left(x,y\right)dx+\ui{CB}P\left(x,y\right)dx+\ui{BA}P\left(x,y\right)dx\right)=\\
 & = & \left\{ \begin{array}{c}
\ui{BA}P\left(x,y\right)dx=\left\{ dx=0\right\} =0\\
\ui{DC}P\left(x,y\right)dx=\left\{ dx=0\right\} =0
\end{array}\right\} =\\
 & = & -\un{\oint}LP\left(x,y\right)dx=\left\{ L=ADCB=\dd D\right\} 
\end{eqnarray*}

\end_inset

 
\end_layout

