\begin_layout Standard
Напомним что для случая одной переменной имеет место разложение по формуле
 Тейлора.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\D F\left(t_{0}\right) & = & F\left(t_{0}+\D t\right)-F\left(t_{0}\right)=\\
 & = & F\pr\left(t_{0}\right)\um\D t+\df{F\ppr\left(t_{0}\right)}{2!}\um\D t^{2}+\tr+\df{F^{\left(n\right)}\left(t_{0}\right)}{n!}+\df{F^{\left(n+1\right)}\left(t_{0}+\Theta\D t\right)}{\left(n+1\right)!}=\\
 & = & dF\left(t_{0}\right)+\df{d^{2}F\left(t_{0}\right)}{2!}+\tr+\df{d^{n}F\left(t_{0}\right)}{n!}+\df{d^{n+1}F\left(t_{0}+\Theta\D t\right)}{\left(n+1\right)!},\ \Theta\in\left(0,1\right)
\end{eqnarray*}

\end_inset

 
\end_layout

\begin_layout Theorem
Пусть в некоторой окрестности точки 
\begin_inset Formula $M_{0}\left(x_{0},y_{0}\right)$
\end_inset

 функция 
\begin_inset Formula $u=f\left(x,y\right)$
\end_inset

 имеет непрерывные частные производные до 
\begin_inset Formula $\left(n+1\right)$
\end_inset

-го порядка.
 Тогда 
\begin_inset Formula $\forall M\left(x_{0}+\D x,y_{0}+\D y\right)$
\end_inset

 имеет место равенство:
\end_layout

\begin_layout Theorem
\begin_inset Formula 
\begin{equation}
\D f\left(x_{0},y_{0}\right)=df\left(x_{0},y_{0}\right)+\df{d^{2}f\left(x_{0},y_{0}\right)}{2!}+\tr+\df{d^{n}f\left(x_{0},y_{0}\right)}{n!}+\df{d^{n+1}f\left(x_{0}+\Theta\D x,y_{0}+\Theta\D y\right)}{\left(n+1\right)!},\ \Theta\in\left(0,1\right)\label{eq:14}
\end{equation}

\end_inset


\end_layout

\begin_layout Proof
Положим 
\begin_inset Formula $x=x_{0}+t\um\D x,\ y=y_{0}+t\um\D y,\ t\in\left(0,1\right)$
\end_inset

 
\begin_inset Formula $\sl$
\end_inset

 
\begin_inset Formula $F\left(t\right)=f\left(x_{0}+t\um\D x,y_{0}+t\um\D y\right)$
\end_inset

 
\begin_inset Formula $\sl$
\end_inset

 
\end_layout

\begin_layout Proof
\begin_inset Formula 
\begin{eqnarray*}
\D f\left(x_{0},y_{0}\right) & = & f\left(x_{0}+\D x,y_{0}+\D y\right)-f\left(x_{0},y_{0}\right)=\\
 & = & F\left(1\right)-F\left(0\right)=\\
 & = & \D F\left(0\right)=\\
 & = & \left\{ \text{По формуле Тейлора для функций многих переменных}\right\} =\\
 & = & dF\left(0\right)+\df{d^{2}F\left(0\right)}{2!}+\tr+\df{d^{n}F\left(0\right)}{n!}+\df{d^{n+1}F\left(\Theta\right)}{\left(n+1\right)!}=\\
 & = & \left\{ \begin{array}{c}
\text{Так как }x=x_{0}+t\um\D x,\ y=y_{0}+t\um\D y\\
\text{линейно зависят от}\ t\ \sl\\
\sl d\tr d^{k}\ \text{обладают инвариантностью формы}
\end{array}\right\} =\\
 & = & (\ref{eq:14})=\\
 & = & df\left(x_{0},y_{0}\right)+\df{d^{2}f\left(x_{0},y_{0}\right)}{2!}+\tr+\df{d^{n}f\left(x_{0},y_{0}\right)}{n!}+\df{d^{n+1}f\left(x_{0}+\Theta\D x,y_{0}+\Theta\D y\right)}{\left(n+1\right)!},\ \Theta\in\left(0,1\right)
\end{eqnarray*}

\end_inset


\end_layout

