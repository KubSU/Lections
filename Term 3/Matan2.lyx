\begin_layout Subsubsection
Сходящиеся последовательности точек в 
\begin_inset Formula $E^{m}$
\end_inset

.
 Критерий Коши сходимости последовательностей.
\end_layout

\begin_layout Definition
Последовательность точек 
\begin_inset Formula $\left\{ x_{n}\right\} =\left\{ x_{1}^{\left(n\right)},x_{2}^{\left(n\right)},\tr,x_{n}^{\left(n\right)}\right\} $
\end_inset

 называется сходящейся в пространстве 
\begin_inset Formula $E^{m}$
\end_inset

 к точке 
\begin_inset Formula $x=\left(x_{1}\tr x_{m}\right)$
\end_inset

 если 
\begin_inset Formula 
\[
\forall\varepsilon>0\quad\exists N=N\left(\varepsilon\right)\quad\forall n\br N\quad\rho\left(x_{n},x\right)<\varepsilon
\]

\end_inset

 или 
\begin_inset Formula 
\[
\sqrt{\ous{i=1}m\left(x_{i}^{\left(n\right)}-x_{i}\right)^{2}}<\varepsilon
\]

\end_inset

записывают при этом 
\begin_inset Formula $\ul{n\st\b}x_{n}=x$
\end_inset

 или 
\begin_inset Formula $x_{n}\st x$
\end_inset

 при 
\begin_inset Formula $n\st\b$
\end_inset

.
 Точка 
\begin_inset Formula $x$
\end_inset

 называется предельной точкой последовательности 
\begin_inset Formula $\left\{ x_{n}\right\} $
\end_inset

.
\end_layout

\begin_layout Lemma
\begin_inset Formula $\ul{n\st\b}x_{n}=x$
\end_inset

 тогда и только тогда, когда 
\begin_inset Formula $x_{i}^{\left(n\right)}\st x_{i}$
\end_inset

 при 
\begin_inset Formula $n\st\b\quad\forall i=\overline{1,m}$
\end_inset

, то есть сходимость в 
\begin_inset Formula $E^{m}$
\end_inset

 --- по координатная.
\end_layout

\begin_layout Proof
Докажем необходимость и достаточность.
\end_layout

\begin_deeper
\begin_layout Enumerate
Необходимость
\end_layout

\begin_deeper
\begin_layout Standard
Пусть 
\begin_inset Formula $\ul{n\st\b}x_{n}=x$
\end_inset

, тогда:
\begin_inset Formula 
\[
\forall\varepsilon>0\quad\exists N=N\left(\varepsilon\right)\quad\forall n\br N\quad\rho\left(x_{n},x\right)<\varepsilon=\sqrt{\ous{i=1}m\left(x_{i}^{\left(n\right)}-x_{i}\right)^{2}}<\varepsilon
\]

\end_inset


\end_layout

\begin_layout Standard
Так как 
\begin_inset Formula $\ous{i=1}m\left(x_{i}^{\left(n\right)}-x_{i}\right)^{2}\br\forall\left|x_{i}^{\left(n\right)}-x_{i}\right|,\quad i=\overline{1,m}$
\end_inset

, то 
\begin_inset Formula $\left|x_{i}^{\left(n\right)}-x_{i}\right|<\varepsilon$
\end_inset

, значит 
\begin_inset Formula 
\[
\ul{n\st\b}x_{i}^{\left(n\right)}=x_{i}\quad\forall i=\overline{1,m}
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Достаточность
\end_layout

\begin_deeper
\begin_layout Standard
Пусть есть координатная сходимость, то есть 
\begin_inset Formula $x_{i}^{\left(n\right)}\overset{n\st\b}{\st}x_{i}\quad\forall i=\overline{1,m}$
\end_inset

, тогда
\begin_inset Formula 
\[
\forall\varepsilon>0\quad\exists N_{i}=N_{i}\left(\varepsilon\right)\quad\forall n\br N_{i}\quad\left|x_{i}^{\left(n\right)}-x_{i}\right|<\df{\varepsilon}?,\quad\forall i=\overline{1,m}
\]

\end_inset

 Сейчас разберемся что там написано вместо 
\begin_inset Formula $?$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\forall n\br N=\max\left\{ N_{1}\tr N_{m}\right\} :\quad\left|x_{i}^{\left(n\right)}-x_{i}\right|<\df{\varepsilon}{\sqrt{m}},\quad\forall i=\overline{1,m}
\]

\end_inset

 Следовательно:
\begin_inset Formula 
\begin{eqnarray*}
\rho\left(x_{n},x\right) & = & \sqrt{\ous{i=1}m\left(x_{i}^{\left(n\right)}-x_{i}\right)^{2}}<\\
 & < & \left|\left(x_{i}^{\left(n\right)}-x_{i}\right)^{2}<\df{\varepsilon^{2}}m\right|<\\
 & < & \df{\varepsilon}{\sqrt{m}}\um\sqrt{m},\quad i=\overline{1,m}
\end{eqnarray*}

\end_inset


\end_layout

\end_deeper
\end_deeper
