\begin_layout Proof
\begin_inset Formula $\oui{\alpha}{\beta}f\left(\varphi\left(t\right),\psi\left(t\right)\right)\varphi\pr\left(t\right)dt=\ous{i=0}{n-1}\oui{t_{i}}{t_{i+1}}f\left(\varphi\left(t\right),\psi\left(t\right)\right)\varphi\pr\left(t\right)dt$
\end_inset


\end_layout

\begin_layout Proof
Оценим модуль разности:
\end_layout

\begin_layout Proof
\begin_inset Note Note
status open

\begin_layout Plain Layout
Hbc 71, 72
\end_layout

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\begin{eqnarray*}
\left|\sigma_{n}-I\right| & = & \left|\ous{i=0}{n-1}\oui{t_{i}}{t_{i+1}}\left[f\left(\xi_{i},\eta_{i}\right)-f\left(\varphi\left(t\right),\psi\left(t\right)\right)\right]\varphi\pr\left(t\right)dt\right|\mr\\
 & \mr & \ous{i=0}{n-1}\left|\oui{t_{i}}{t_{i+1}}f\left(\varphi\left(t_{i}\right),\psi\left(t_{i}\right)\right)-f\left(\varphi\left(t\right),\psi\left(t\right)\right)\right|\um\left|\varphi\pr\left(t\right)\right|dt\mr\\
 & \mr & \left\{ \begin{array}{c}
\varphi\pr\left(t\right)\ -\ \text{непрерывна}\ \sl\\
\ \text{ограничена на}\ \left[\alpha;\beta\right]\ \sl\\
\exists K>0\ \left|\varphi\pr\left(t\right)\right|\mr K\ \forall t\in\left[\alpha;\beta\right]\\
\text{Так как}\ f\left(x,y\right)\ \text{непрерывна на }L,\\
\text{то}\ f\left(\varphi\left(t\right),\psi\left(t\right)\right)\ -\ \text{непрерывная}\\
\text{сложная функция на }\left[\alpha;\beta\right]\sl\\
\text{колебания}\ \omega_{i}=M_{i}-m_{i}\ \text{функции}\\
f\left(\varphi\left(t\right),\psi\left(t\right)\right)\ \text{на отрезке }\left[t_{i};t_{i+1}\right]\\
\text{можно сделать меньше чем}\ \df{\varepsilon}{K\left(\beta-\alpha\right)},\\
\text{если}\ \D t_{i}\ \text{достаточно мало. Поэтому}\\
\exists\delta=\delta\left(\varepsilon\right):\ \lambda=\max\D t_{i}<\delta_{i}\sl\omega_{i}<\df{\varepsilon}{K\left(\beta-\alpha\right)}\\
\sl\left|\oui{t_{i}}{t_{i+1}}f\left(\varphi\left(t_{i}\right),\psi\left(t_{i}\right)\right)-f\left(\varphi\left(t\right),\psi\left(t\right)\right)\right|\mr\omega_{i}<\df{\varepsilon}{K\left(\beta-\alpha\right)}
\end{array}\right\} \mr\\
 & \mr & k\df{\varepsilon}{K\left(\beta-\alpha\right)}\ous{i=0}{n-1}\oui{t_{i}}{t_{i+1}}dt=\\
 & = & \left\{ \ous{i=0}{n-1}\oui{t_{i}}{t_{i+1}}dt=\beta-\alpha\right\} =\\
 & = & \varepsilon
\end{eqnarray*}

\end_inset


\end_layout

