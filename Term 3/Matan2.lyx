\begin_layout Standard
Для сложения функции 
\begin_inset Formula $u$
\end_inset

 аргументов 
\begin_inset Formula $t_{1}\tr t_{k}$
\end_inset

 получим:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
du & = & \df{\dd u}{\dd t_{1}}\um dt_{1}+\df{\dd u}{\dd t_{2}}\um dt_{2}+\tr+\df{\dd u}{\dd t_{k}}\um dt_{k}=\\
 & = & \left\{ \text{Дифференциал сложных функций}\right\} =\\
 & = & \left(\df{\dd u}{\dd x_{1}}\um\df{\dd x_{1}}{\dd t_{1}}+\df{\dd u}{\dd x_{2}}\um\df{\dd x_{2}}{\dd t_{1}}+\tr+\df{\dd u}{\dd x_{m}}\um\df{\dd x_{m}}{\dd t_{1}}\right)\um dt_{1}+\\
 & + & \left(\df{\dd u}{\dd x_{1}}\um\df{\dd x_{1}}{\dd t_{2}}+\df{\dd u}{\dd x_{2}}\um\df{\dd x_{2}}{\dd t_{2}}+\tr+\df{\dd u}{\dd x_{m}}\um\df{\dd x_{m}}{\dd t_{2}}\right)\um dt_{2}+\\
 & + & \tr+\\
 & + & \left(\df{\dd u}{\dd x_{1}}\um\df{\dd x_{1}}{\dd t_{k}}+\df{\dd u}{\dd x_{2}}\um\df{\dd x_{2}}{\dd t_{k}}+\tr+\df{\dd u}{\dd x_{m}}\um\df{\dd x_{m}}{\dd t_{k}}\right)\um dt_{k}=\\
 & = & \df{\dd u}{\dd x_{1}}\um\left(\df{\dd x_{1}}{\dd t_{1}}\um dt_{1}+\df{\dd x_{1}}{\dd t_{2}}\um dt_{2}+\tr+\df{\dd x_{1}}{\dd t_{k}}\um dt_{k}\right)+\\
 & + & \tr+\\
 & + & \df{\dd u}{\dd x_{m}}\um\left(\df{\dd x_{m}}{\dd t_{1}}\um dt_{1}+\df{\dd x_{m}}{\dd t_{2}}\um dt_{2}+\tr+\df{\dd x_{m}}{\dd t_{k}}\um dt_{k}\right)=\\
 & = & \left\{ \begin{array}{c}
\df{\dd x_{1}}{\dd t_{1}}\um dt_{1}+\df{\dd x_{1}}{\dd t_{2}}\um dt_{2}+\tr+\df{\dd x_{1}}{\dd t_{k}}\um dt_{k}=dx_{1}\\
\tr\\
\df{\dd x_{m}}{\dd t_{1}}\um dt_{1}+\df{\dd x_{m}}{\dd t_{2}}\um dt_{2}+\tr+\df{\dd x_{m}}{\dd t_{k}}\um dt_{k}=dx_{m}
\end{array}\right\} =\\
 & = & \df{\dd u}{\dd x_{1}}\um dx_{1}+\tr+\df{\dd u}{\dd x_{m}}\um dx_{m}
\end{eqnarray*}

\end_inset

 
\end_layout

