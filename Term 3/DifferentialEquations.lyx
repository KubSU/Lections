\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
L\left(e^{\alpha x}\right)=P\left(\alpha\right)e^{\alpha x}\ \text{и}L\left(x^{m}e^{\alpha x}\right)=\ous{j=0}mC_{m}^{j}\df{d^{j}P\left(\alpha\right)}{d\lambda}x^{m-j}e^{\alpha x}\label{eq:j8}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
L\left(x^{k}Q_{m}\left(x\right)e^{\alpha x}\right) & = & L\left(\ous{s=0}mq_{s}x^{m+k-s}e^{\alpha x}\right)=\\
 & = & \left\{ \text{Так как оператор линейный}\right\} =\\
 & = & \ous{s=0}mq_{s}L\left(m^{m+k-s}e^{\alpha x}\right)=\\
 & = & \left\{ \text{По равенствам \ref{eq:j8}}\right\} =\\
 & = & \ous{s=0}mq_{s}\ous{j=0}{m+k-s}C_{m+k-s}^{j}\dif dj{P\left(\alpha\right)}{\lambda}\um x^{m+k-s-j}e^{\alpha x}=\\
 & = & \left\{ \text{В силу \ref{eq:j6}}\right\} =\\
 & = & \ous{s=0}mq_{s}\ous{j=k}{m+k-s}C_{m+k-s}^{j}\dif dj{P\left(\alpha\right)}{\lambda}\um x^{m+k-s-j}e^{\alpha x}=\\
 & = & \left\{ \begin{array}{c}
\text{Заменим переменную по которой}\\
\text{производится}\text{сумирование}\\
l=j-k\\
j=l+k
\end{array}\right\} =\\
 & = & \ous{s=0}mq_{s}\ous{l=0}{m-s}C_{m+k-s}^{l+k}\dif d{l+k}{P\left(\alpha\right)}{\lambda}x^{m-s-l}e^{\alpha x}\\
 & = & \ous{s=0}mP_{s}x^{m-s}e^{\alpha x}
\end{eqnarray*}

\end_inset


\end_layout

