\begin_layout Standard
Момент импульса твердого тела (
\begin_inset Formula $\v M$
\end_inset

) = момент импульса поступательного движения (
\begin_inset Formula $\v{R_{c}}\x\v J$
\end_inset

) + собственный механический момент (
\begin_inset Formula $\v{M_{c}}$
\end_inset

).
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\v{M_{c}} & = & \ous{i=1}n\v{r_{i}'}\x\left(m_{i}\v{v_{i}'}\right)=\\
 & = & \ous{i=1}nm_{i}\um\v{r_{i}'}\x\left(\v{\omega}\x\v{r_{i}}\right)=\\
 & = & \left\{ \v a\x\left(\v b\x\v c\right)=\v b\left(\v a;\v c\right)-\v c\left(\v a;\v b\right)\right\} =\\
 & = & \ous{i=1}nm_{i}\left(\v{r_{i}'}^{2}\v{\omega}-\left(\v{r_{i}'};\v{\omega}\right)\um\v{r_{i}'}\right)
\end{eqnarray*}

\end_inset

 
\end_layout

\begin_layout Standard
Возьмем покомпонентно:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
M_{C_{x}} & = & \ous{i=1}n\left(m_{i}\left(x_{i}^{2}+y_{i}^{2}+z_{i}^{2}\right)\omega_{x}-\left(x_{i}\omega_{x}+y_{i}\omega_{y}+z_{i}\omega_{z}\right)x\right)=\\
 & =- & \ous{i=1}nm_{i}\left(y_{i}^{2}+z_{i}^{2}\right)-\ous{i=1}nm_{i}y_{i}x_{i}\omega_{y}-\ous{i=1}nm_{i}z_{i}x_{i}\omega_{z}=\\
 & = & \left\{ \begin{array}{c}
-\ous{i=1}nm_{i}\left(y_{i}^{2}+z_{i}^{2}\right)=I_{xx}\\
-\ous{i=1}nm_{i}y_{i}x_{i}\omega_{y}=I_{xy}\\
-\ous{i=1}nm_{i}z_{i}x_{i}\omega_{z}=I_{xz}
\end{array}\right\} 
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\v{M_{C_{x}}}=I_{xx}\omega_{x}+I_{xy}\omega_{y}+I_{xz}\omega_{z}$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $\v{M_{C_{y}}}=I_{xy}\omega_{x}+I_{yy}\omega_{y}+I_{yz}\omega_{z}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\v{M_{C_{z}}}=I_{xz}\omega_{x}+I_{xy}\omega_{y}+I_{zz}\omega_{z}$
\end_inset


\end_layout

