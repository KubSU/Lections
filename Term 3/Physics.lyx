\begin_inset Formula 
\begin{eqnarray*}
\vec{M_{o}} & = & \ous{i=1}n\vec{r_{i}}\x\vec{J_{i}}=\\
 & = & \ous{i=1}nm_{i}\left(\vec{r_{i}'}+\vec{r_{c}}\right)\x\left(\vec{v_{i}'}+\vec{v_{c}}\right)=\\
 & = & \ous{i=1}nm_{i}\vec{r_{i}'}\x\vec{v_{i}'}+\left(\ous{i=1}nm_{i}\vec{r_{i}'}\right)\x\vec{v_{c}}+\vec{r_{c}}\x\left(\ous{i=1}nm_{i}\vec{v_{i}'}\right)+\vec{r_{c}}\x\vec{v_{c}}\left(\ous{i=1}nm_{i}\right)=\\
 & = & \left\{ \begin{array}{c}
\vec{r_{c}}=\df{\sum m_{i}\vec{r_{i}}}{\sum m_{i}}\\
M\vec{r_{0}}=\sum m_{i}\vec{r_{i}}\\
\left(\ous{i=1}nm_{i}\vec{r_{i}'}\right)=0\\
\left(\ous{i=1}nm_{i}\vec{v_{i}'}\right)=0
\end{array}\right\} =\\
 & = & \ous{i=1}n\vec{r_{i}'}\x\vec{J_{i}'}+\vec{r_{c}}\x M\vec{v_{c}}=\\
 & = & \left\{ \begin{array}{c}
\ous{i=1}n\vec{r_{i}'}\x\vec{J_{i}'}=M_{c}'\\
\vec{r_{c}}\x M\vec{v_{c}}=\vec{r_{c}}\x\vec J
\end{array}\right\} =\\
 & = & \vec{M_{c}'}+\vec{r_{c}}\x\vec J
\end{eqnarray*}

