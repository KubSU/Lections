\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
x\left(t\right) & = & Re^{i\alpha}e^{\left(\mu+i\nu\right)t}\df 12\left(h^{\left(1\right)}-ih^{\left(2\right)}\right)+Re^{-i\alpha}e^{\left(\mu-i\nu\right)}\df 12\left(h^{\left(1\right)}+ih^{\left(2\right)}\right)=\\
 & = & Re^{\mu t}\df{e^{i\left(\alpha+\nu t\right)}+e^{-i\left(\alpha+\nu t\right)}}2h^{\left(1\right)}-iRe^{\mu t}\df{e^{i\left(\alpha+\nu t\right)}-e^{-i\left(\alpha+\mu t\right)}}2h^{\left(2\right)}=\\
 & = & Re^{\mu t}\cos\left(\alpha+\nu t\right)h^{\left(1\right)}+Re^{\mu t}\sin\left(\alpha+\nu t\right)h^{\left(2\right)}
\end{eqnarray*}

\end_inset


\end_layout

