\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\df{d\ov z{\sim}}{dx}=\ov B{\sim}\ov z{\sim}+\ov g{\sim}\left(x\right)\label{eq:_1_3_3}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Система (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:_1_3_3"

\end_inset

